@font-face {
  font-family: octicons-anchor;
  src: url(data:font/woff;charset=utf-8;base64,) format('woff');
}
.rich-text-container {
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
  max-width: 100%;
  font: normal normal 24rpx octicons-anchor;
}
.wxmarked-p{
  letter-spacing: 1px !important;
  line-height: 1.7 !important;
  min-height: 1em !important;
  box-sizing: border-box !important;
  word-wrap: break-word !important;
  text-align: justify;
  margin: 20rpx 0 !important;
}

.wxmarked-image {
  max-width: 80%;
}

.wxmarked-header {
  /* word-break: break-all !important; */
  margin: 20rpx 0;
  line-height: 1.2;
  text-align: left;
  font-weight: bold;
  /* padding-left: 30rpx; */
}
.wxmarked-header-1 {
  border-left: 6px solid #71BA51 !important;
  font-size: 22rpx !important;
}
.wxmarked-header-2 {
  border-left: 4px solid #71BA51 !important;
  font-size: 20rpx !important;
}
.wxmarked-header-3 {
  border-left: 4px solid #71BA51 !important;
  font-size: 16rpx !important;
}

.wxmarked-strong {
  font-weight: normal;
  color: #FD5B03;
}

.wxmarked-list {
  padding-left: 30px;
}
.wxmarked-listitem {
  line-height: 24px;
}

.wxmarked-blockquote {
  border-left: 10rpx solid rgba(128,128,128,0.075);
  background-color: rgba(128,128,128,0.05);
  padding: 10rpx 16rppx !important;
  margin: 0rpx;
}
.wxmarked-blockquote p {
  color: #898989;
  margin: 0px;
}
.wxmarked-pre {
  background-color: #f8f8f8;
  border-radius: 3px;
  word-wrap: break-word;
  padding: 12px 13px;
  font-size: 13px;
  color: #898989;
}

.wxmarked-hr {
  height: 4rpx;
  padding: 0;
  margin: 16rpx 0;
  background-color: #e7e7e7;
  border: 0 none;
  overflow: hidden;
  box-sizing: content-box;
  border-bottom: 2rpx solid #ddd;
}
.wxmarked-code {
  color: #a71d5d;
  padding: 0.2em 0.4em;
  margin: 0;
  font-size: 85%;
  background-color: rgba(27,31,35,0.05);
  border-radius: 3px;
}
.wxmarked-a {
  color: #4183C4 !important;
  text-decoration: none !important;
}
.wxmarked-table {
  border-collapse: collapse;
  border-spacing: 0;
  border: 1px solid #c6cbd1;
}
.wxmarked-td {
   border-right: 1px solid #c6cbd1; 
}
.wxmarked-tr {
  background-color: #fff;
   border-bottom: 1px solid #c6cbd1; 
}
.wxmarked-tr:nth-child(2n) {
  background-color: #f6f8fa;
}




